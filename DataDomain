using System.Runtime.Serialization;
using System.IO;
using System.Text;

namespace ConvertJSON
{
    [DataContract]
    public class Ticker
    {
        [DataMember]
        public decimal buy { get; set; }

        [DataMember]
        public decimal sell { get; set; }

        [DataMember]
        public decimal high { get; set; }

        [DataMember]
        public decimal low { get; set; }

        [DataMember]
        public decimal vol { get; set; }

        [DataMember]
        public decimal last { get; set; }

        [DataMember]
        public int date { get; set; }

        public Ticker()
        {
            this.buy = 0.0m;
            this.sell = 0.0m;
            this.high = 0.0m;
            this.low = 0.0m;
            this.vol = 0.0m;
            this.last = 0.0m;
            this.date = 0;
        }

    }
}
